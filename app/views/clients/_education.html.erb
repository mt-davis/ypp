<div>
	<!-- Nav tabs -->
	<ul class="nav nav-tabs" role="tablist">
	<!-- <li role="presentation" class="active"><a href="#education" aria-controls="education" role="tab" data-toggle="tab">Education</a>
		</li> -->
		<li role="presentation" class="active"><a href="#employment" aria-controls="employment" role="tab" data-toggle="tab">Labor</a>
		</li>
		<li role="presentation"><a href="#reproductiveHealth" aria-controls="reproductiveHealt" role="tab" data-toggle="tab">Sexual Health</a>
		</li>
		<li role="presentation"><a href="#mentalhealth" aria-controls="mentalhealth" role="tab" data-toggle="tab">Mental Health</a>
		</li>
		<li role="presentation"><a href="#parents" aria-controls="parents" role="tab" data-toggle="tab">Parents</a>
		</li>
	</ul>
	<!-- Tab panes -->
	<div class="tab-content">
		<div role="tabpanel" class="tab-pane active" id="employment">
					<div class="container-fluid">
			   <div class="row">
			      <div class="col-md-12">
			         <label for="grade">
			         <%=f .label :employment_id, "Employment" %>
			         </label>
			         <%=f .collection_select :employment_id, Employment.all, :id, :name, {prompt: "Employment Status"}, class: "btn btn-info", class: "pure-input-24-24" %>
			         <!--<div class="pure-u-1 pure-u-md-1-3"> -->
			            <label for="wk">
			            <%=f .label :wk_id, "Working Where?" %>
			            </label>
			            <%=f .text_area :wk_id, cols: "20", rows: "1" %>
			        <!-- </div> -->
			      </div>
			   </div>
			</div>
		</div>
		<div role="tabpanel" class="tab-pane" id="reproductiveHealth">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-6">
						<div class="pure-form pure-form-aligned">
							<%=f .collection_check_boxes :rhealth_ids, Rhealth.all, :id, :name do |b| %>
								<div class="pure-control-group">
									<%=b.check_box class: "rh-input" %>
										<%=b.label %>
								</div>
								<% end %>
						</div>
						<!-- End of grid -->
					</div>
					<!-- End of reproductiveHealth -->
					<div class="col-md-6">
						<!-- secnod row -->
						...
					</div>
				</div>
			</div>
			<!-- End of grid -->
		</div>
		<!-- End of reproductiveHealth -->
		<div role="tabpanel" class="tab-pane" id="mentalhealth">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">
					<div class="mhealthsub">
						<label>
							<%= f.check_box :msub %> Are you or others concerned that you have a problem with any of the following substances?
					
						</label>
						</br>
						</div>
						<div "container">
							<div class="subtance">
								<div class="mhealth">
									<label for="cigarette">
										<%=f .label :cigarette_id, "Smoked?" %>
									</label>
									<%=f .collection_select :cigarette_id, Cigarette.all, :id, :name, {prompt: "Select"}, class: "btn btn-info", class: "pure-input-4-24" %>
								</div>
								<div class="mhealth">
									<label for="alcohol">
										<%=f .label :alcohol_id, "Alcohol?" %>
									</label>
									<%=f .collection_select :alcohol_id, Alcohol.all, :id, :name, {prompt: "Select"}, class: "btn btn-info", class: "pure-input-4-24" %>
								</div>
								<div class="mhealth">
									<label for="race">
										<%=f .label :mj_id, "Marijuana?" %>
									</label>
									<%=f .collection_select :mj_id, Mj.all, :id, :name, {prompt: "Select"}, class: "btn btn-info", class: "pure-input-4-24" %>
								</div>
							</div>
							<!-- second row -->
							<div class="subtance">
								<div class="mhealth">
									<label for="od">
										<%=f .label :od_id, "Other Drugs" %>
									</label>
									<%=f .collection_select :od_id, Cigarette.all, :id, :name, {prompt: "Select"}, class: "btn btn-info", class: "pure-input-4-24" %>
								</div>
								<div class="mhealth">
									<label for="otc">
										<%=f .label :otc_id, "OTC, Prescibed, Street?" %>
									</label>
									<%=f .collection_select :otc_id, Otc.all, :id, :name, {prompt: "Select"}, class: "btn btn-info", class: "pure-input-4-24" %>
								</div>
							</div>
							<!-- end of row -->
							<div class="container">
								<div class="row">
									<div class="col-md-12">
										<div class="pure-form pure-form-aligned">
											<h3>Withnessed or been a victim of any of the folllowing?</h3>
											<%=f .collection_check_boxes :mhealth_ids, Mhealth.all, :id, :name do |b| %>
												<div class="pure-control-group">
													<%=b.check_box class: "rh-input" %>
														<%=b.label %>
												</div>
												<% end %>
													<div class "container">
														<p>If witness to other trauma, what kind?
														</p>
														<label for="truma">
														</label>
														<%=f .text_area :truma_id, cols: "20", rows: "1" %>
													</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- End of Mental Health -->
		<div role="tabpanel" class="tab-pane" id="parents">
			<h3 class="parents">Current Status of Parents?</h3>
			<div class="container">
				<div class="row">
					<div class="col-md-6">
						<div class="pure-form pure-form-aligned">
							<%=f.collection_check_boxes :cparent_ids, Cparent.all, :id, :name do |b| %>
								<div class="pure-control-group">
									<%=b.check_box class: "rh-input" %>
										<%=b.label %>
								</div>
								<% end %>
						</div>
						<!-- End of grid -->
					</div>
					<!-- End of mentalhealth -->
					<div class="col-md-6">
						<!-- secnod row -->
						...
					</div>
				</div>
			</div>
			<!-- End of grid -->
		</div>
	</div>
</div>