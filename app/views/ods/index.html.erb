<p id="notice"><%= notice %></p>

<h1>Listing Ods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ods.each do |od| %>
      <tr>
        <td><%= od.name %></td>
        <td><%= link_to 'Show', od %></td>
        <td><%= link_to 'Edit', edit_od_path(od) %></td>
        <td><%= link_to 'Destroy', od, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Od', new_od_path %>
