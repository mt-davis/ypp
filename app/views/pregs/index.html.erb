<p id="notice"><%= notice %></p>

<h1>Listing Pregs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pregs.each do |preg| %>
      <tr>
        <td><%= preg.name %></td>
        <td><%= link_to 'Show', preg %></td>
        <td><%= link_to 'Edit', edit_preg_path(preg) %></td>
        <td><%= link_to 'Destroy', preg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preg', new_preg_path %>
