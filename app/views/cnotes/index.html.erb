<p id="notice"><%= notice %></p>

<h1>Listing Cnotes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cnotes.each do |cnote| %>
      <tr>
        <td><%= cnote.description %></td>
        <td><%= link_to 'Show', cnote %></td>
        <td><%= link_to 'Edit', edit_cnote_path(cnote) %></td>
        <td><%= link_to 'Destroy', cnote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cnote', new_cnote_path %>
